{
	"manifest_version": 2,
	"name": "github repo related",
	"version": "0.0.1",
	"background": { "scripts": ["/js/jquery-2.0.0.min.js", "/js/background.js"] },
	"page_action": {
		"default_icon": {
			"19": "/img/logo.png",
			"38": "/img/logo.png"
		},
		"default_title": "a github plugin",
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions" : ["tabs", "http://localhost/", "https://github.com/	"],
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	}
}
